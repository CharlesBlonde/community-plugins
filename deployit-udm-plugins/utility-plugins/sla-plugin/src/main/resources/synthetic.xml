<synthetic xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://www.xebialabs.com/deployit/synthetic"
           xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd">


    <type-modification type="udm.Environment">
        <property label="SLA Starts At Time" name="slaStartsAtTime" required="false" category="SLA"
                  description="enters the hour when the SLA starts (format HH:mm)"/>
        <property name="startFallBackMode" kind="enum" enum-class="ext.deployit.community.plugin.sla.ci.FallbackMode" category="SLA" default="CONTINUE"/>

        <property label="SLA Ends At Time" name="slaEndsAtTime" required="false" category="SLA"
                  description="enters the hour when the SLA ends (format HH:mm)"/>
        <property name="endFallBackMode" kind="enum" enum-class="ext.deployit.community.plugin.sla.ci.FallbackMode" category="SLA" default="PAUSE"/>

        <property name="timePattern" hidden="true" default="HH:mm"/>
    </type-modification>


</synthetic>
