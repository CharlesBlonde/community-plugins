<?xml version="1.0" encoding="UTF-8"?>
<synthetic xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://www.xebialabs.com/deployit/synthetic"
           xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd">

    <type-modification type="udm.Environment">
        <property name="requiresChangeTicket" kind="boolean" required="false" default="false" category="Deployment Checklist" />
    </type-modification>
        
    <type-modification type="udm.DeploymentPackage">
        <!-- will be set automatically during deployment planning, not by users -->
        <property name="satisfiesChangeTicket" kind="boolean" required="false" default="false" category="Deployment Checklist" />
    </type-modification>
</synthetic>