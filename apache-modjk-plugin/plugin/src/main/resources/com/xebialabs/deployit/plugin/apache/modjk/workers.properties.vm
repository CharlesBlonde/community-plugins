#foreach($worker in $workers)
#Configuration worker $worker.name
worker.${worker.name}.type=$worker.type
worker.${worker.name}.host=$worker.address
worker.${worker.name}.port=$worker.port
	#if ($worker.cacheSize > 0)
worker.${worker.name}.cachesize=$worker.cacheSize
	#end
	#if ($worker.cacheTimeout > 0)
worker.${worker.name}.cache_timeout=$worker.cacheTimeout
	#end
	#if ($worker.socketKeepAlive)
worker.${worker.name}.socket_keepalive=1
#end
	#if ($worker.socketTimeout > 0)
worker.${worker.name}.socket_timeout=$worker.socketTimeout
	#end

#end

#foreach($loadbalancer in $loadbalancers)
#Configuration loadbalancer $loadbalancer.name
worker.${loadbalancer.name}.type=lb
worker.${loadbalancer.name}.balance_workers=$loadbalancer.workerNames
worker.${loadbalancer.name}.sticky_session=$loadbalancer.stickySession

#end

#if(${pluginConfiguration.jkstatus})
worker.list=jkstatus,$loadbalancerNames
worker.jkstatus.type=status
#else
worker.list=$loadbalancerNames
#end